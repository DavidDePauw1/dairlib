group "0.operator" {
    cmd "1.drake-director" {
        exec = "bazel-bin/director/drake-director --script examples/kuka_iiwa_arm/signal_scope_panel.py --script director/scripts/VisualizationGUI.py director/scripts/cassie_test.json";
        host = "localhost";
    }
}

group "1.simulated-robot" {
    cmd "1.simulator" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_simulation";
        host = "localhost";
    }

    cmd "1b.simulator with recording" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_simulation_recording";
        host = "localhost";
    }

    cmd "1a. rbp simulator" {
        exec = "../drake/bazel-bin/examples/kuka_iiwa_arm/kuka_simulation";
        host = "localhost";
    }

    cmd "2.position_broadcaster" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/iiwa_controller_demo";
        host = "localhost";
    }

    cmd "3.inv_demo" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_inv_demo";
        host = "localhost";
    }

    cmd "3a.crIK_demo" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_crIK_demo";
        host = "localhost";
    }

    cmd "3b.manip_IK_push_demo" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/manip_push_test";
        host = "localhost";
    }

    cmd "3b.manip_Jac_push_demo" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/manip_push_test_Jac";
        host = "localhost";
    }


    cmd "4.iiwa_oscillate" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/iiwa_oscillate";
        host = "localhost";
    }

    cmd "5.Visualizer" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/iiwa_visualizer";
        host = "localhost";
    }

    cmd "LCM Physical Interface" {
        exec = "../drake-iiwa-driver/build/bin/kuka_driver --restart_fri";
        host = "localhost";
    }

    cmd "6.E-Stop" {
        exec = "bazel-bin/examples/kuka_iiwa_arm/iiwa_estop";
        host= "localhost";
    }
}

group "2.lcm-tools" {
    cmd "0.lcm-spy" {
        exec = "bazel-bin/lcmtypes/dair-lcm-spy";
        host = "localhost";
    }
    cmd "1.signal-scope" {
        exec = "bazel-bin/director/signal-scope";
        host = "localhost";
    }
}
